{
  "version": 2,
  "builds": [
    {
      "src": "server/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "server/app.py"
    },
    {
      "src": "/predict",
      "dest": "server/app.py"
    },
    {
      "src": "/plot",
      "dest": "server/app.py"
    },
    {
      "src": "/data/(.*)",
      "dest": "server/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "server/app.py"
    }
  ]
}
